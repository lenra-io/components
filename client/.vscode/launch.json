{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run DevTool",
            "program": "lib/dev_tool.dart",
            "request": "launch",
            "flutterMode": "debug",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-port",
                "10000",
                "--dart-define=LENRA_SERVER_URL=http://localhost:4000"
            ]
        },
        {
            "name": "Run Web Dev",
            "program": "lib/main.dart",
            "request": "launch",
            "flutterMode": "debug",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-port",
                "10000",
                "--dart-define=LENRA_SERVER_URL=http://localhost:4000"
            ]
        },
        {
            "name": "Run Backoffice Web Dev",
            "program": "lib/main.dart",
            "request": "launch",
            "flutterMode": "debug",
            "type": "dart",
            "args": [
                "-d",
                "chrome",
                "--web-port",
                "10000",
                "--dart-define=LENRA_SERVER_URL=http://localhost:4000",
                "--dart-define=LENRA_APPLICATION=dev"
            ]
        },
        {
            "name": "Run Dev",
            "program": "lib/main.dart",
            "request": "launch",
            "flutterMode": "debug",
            "type": "dart",
            "args": [
                "--web-port",
                "10000",
                "--dart-define=LENRA_SERVER_URL=${env:LENRA_SERVER_URL}",
                "--dart-define=LENRA_BASIC_AUTH=${env:LENRA_BASIC_AUTH}"
            ]
        },
        {
            "name": "Run Backoffice Dev",
            "program": "lib/main.dart",
            "request": "launch",
            "flutterMode": "debug",
            "type": "dart",
            "args": [
                "--web-port",
                "10000",
                "--dart-define=LENRA_SERVER_URL=${env:LENRA_SERVER_URL}",
                "--dart-define=LENRA_BASIC_AUTH=${env:LENRA_BASIC_AUTH}",
                "--dart-define=LENRA_APPLICATION=dev"
            ]
        },
        {
            "name": "Run Release",
            "program": "lib/main.dart",
            "request": "launch",
            "flutterMode": "release",
            "type": "dart",
            "args": [
                "--dart-define=LENRA_SERVER_URL=${env:LENRA_SERVER_URL}",
                "--dart-define=LENRA_BASIC_AUTH=${env:LENRA_BASIC_AUTH}"
            ]
        },
        {
            "name": "Test",
            "type": "dart",
            "request": "launch",
            "program": "./test/",
            "args": []
        }
    ]
}